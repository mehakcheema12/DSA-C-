#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter side length of square: ";
    cin >> n;
    
    // Outer loop for rows
    for(int i = 1; i <= n; i++) {
        // Inner loop for columns
        for(int j = 1; j <= n; j++) {
            cout << "* ";
        }
        cout << endl; // Move to next line after each row
    }
    return 0;
}
